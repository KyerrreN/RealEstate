{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog", "Serilog.Exceptions" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/serilog.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Swagger": {
    "Title": "Real Estate API",
    "Version": "v1",
    "Description": "API for managing real estate bookings, users, and reviews."
  },
  "Auth0": {
    "Domain": "",
    "Audience": "",
    "ClientSecret": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "PostgreSQL": {
    "ConnectionString": ""
  },
  "MassTransit": {
    "Username": "",
    "Password": "",
    "Host": ""
  }
}
